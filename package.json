{
  "name": "youtube-theme",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config vite.client.config.ts",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite --config vite.client.config.ts build",
    "build:admin": "vue-tsc --noEmit && APP_MODE=admin vite --config vite.admin.config.ts build",
    "lint": "eslint --ext .ts,vue ."
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.175",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-typescript": "^8.0.0",
    "autoprefixer": "^10.3.7",
    "eslint": "^8.0.1",
    "eslint-plugin-vue": "^7.19.1",
    "postcss": "^8.3.9",
    "sass": "^1.43.2",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}
